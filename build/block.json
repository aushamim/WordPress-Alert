{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "alert/alert",
  "version": "0.1.0",
  "title": "Alert",
  "category": "widgets",
  "icon": "admin-comments",
  "description": "Custom Alert",
  "example": {},
  "supports": {
    "interactivity": true
  },
  "textdomain": "alert",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js",
  "attributes": {
    "text": {
      "type": "string",
      "default": ""
    },
    "classes": {
      "type": "string",
      "default": "alert-container alert-default"
    },
    "class": {
      "type": "string",
      "default": "alert-default"
    }
  }
}